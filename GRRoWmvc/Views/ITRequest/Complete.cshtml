@model GRRoWmvc.Models.ITRequestViewModels.ITRequestCompleteViewModel

<h4>Complete IT Request</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Complete">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CompletionNotes" class="control-label"></label>
                <input asp-for="CompletionNotes" class="form-control" />
                <span asp-validation-for="CompletionNotes" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Complete" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="List">Back to List</a>
</div>

